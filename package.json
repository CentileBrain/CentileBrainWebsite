    "eslint-fix": "eslint --no-error-on-unmatched-pattern \"src/**/*.{js,jsx}\" --fix",
    "fix": "npm run prettier-fix && npm run eslint-fix",
    "lint": "eslint --ext .js,.jsx src/",
    "prettier-check": "prettier --config ./.prettierrc.json --check \"./src/**/*.{js,jsx,css,less}\"",
    "prettier-fix": "prettier --config ./.prettierrc.json --write \"./src/**/*.{js,jsx,css,less}\"",
    "start": "dora --plugins webpack",
    "test": "atool-test-mocha ./**/__tests__/*-test.js"
  }
